class Solution {
    public int[][] transpose(int[][] matrix) {
        int n=matrix.length;
        int len=matrix[0].length;
        int result[][]=new int[len][n];
        for(int i=0; i<len; i++){
            for(int j=0; j<n; j++){
                result[i][j]=matrix[j][i];

            }
        }
        return result;
    }
}
